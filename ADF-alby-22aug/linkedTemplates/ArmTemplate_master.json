{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-alby-22aug"
		},
		"LS_GCP_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'LS_GCP'"
		},
		"LS_StorageAccount_Gen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_StorageAccount_Gen2'"
		},
		"LS_GCP2_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'LS_GCP2'"
		},
		"LS_AWS_S3_properties_typeProperties_accessKeyId": {
			"type": "object",
			"defaultValue": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "AzureKeyVault1",
					"type": "LinkedServiceReference"
				},
				"secretName": "Acess-key"
			}
		},
		"LS_GCP_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "GOOG1EQQO53SHVSBN6M22ZMOTCW3CMBGZ3JYNNFCVNG2WXLNSJZFFZVLVFWA3"
		},
		"LS_StorageAccount_Gen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://saalby.dfs.core.windows.net/"
		},
		"LS_GCP2_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "GOOG1EQQO53SHVSBN6M22ZMOTCW3CMBGZ3JYNNFCVNG2WXLNSJZFFZVLVFWA3"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-alby.vault.azure.net/"
		},
		"GoogleCloudStorage1_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "GOOG1EQQO53SHVSBN6M22ZMOTCW3CMBGZ3JYNNFCVNG2WXLNSJZFFZVLVFWA3"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-alby-22aug_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_GCP_secretAccessKey": {
						"value": "[parameters('LS_GCP_secretAccessKey')]"
					},
					"LS_StorageAccount_Gen2_accountKey": {
						"value": "[parameters('LS_StorageAccount_Gen2_accountKey')]"
					},
					"LS_GCP2_secretAccessKey": {
						"value": "[parameters('LS_GCP2_secretAccessKey')]"
					},
					"LS_GCP_properties_typeProperties_accessKeyId": {
						"value": "[parameters('LS_GCP_properties_typeProperties_accessKeyId')]"
					},
					"LS_StorageAccount_Gen2_properties_typeProperties_url": {
						"value": "[parameters('LS_StorageAccount_Gen2_properties_typeProperties_url')]"
					},
					"LS_GCP2_properties_typeProperties_accessKeyId": {
						"value": "[parameters('LS_GCP2_properties_typeProperties_accessKeyId')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"LS_AWS_S3_properties_typeProperties_accessKeyId": {
						"value": "[parameters('LS_AWS_S3_properties_typeProperties_accessKeyId')]"
					},
					"GoogleCloudStorage1_properties_typeProperties_accessKeyId": {
						"value": "[parameters('GoogleCloudStorage1_properties_typeProperties_accessKeyId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}